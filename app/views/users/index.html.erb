<div id="users">
<% for user in @users %>
  <div class="user">
    <p>
      <strong><%=h user.name %></strong>
      <%= link_to "Add Friend", friendships_path(:friend_id => user), :method => :post %>
      <div class="clear"></div>
    </p>
  </div>
<% end %>
</div>


<% @users.each do |user| %>
  <%= user.name %>
<% end %>
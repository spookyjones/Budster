<% @user_friends.each do |f| %>
	<% if f %>
		<%= f.name %>
		<% unless current_user.follows?(f)%>
			<%= link_to "Follow", user_follow_path(:user_id => f.id), :method => :post %>
		<% else %>
			<%= link_to "Unfollow", user_unfollow_path(:user_id => f.id), :method => :post %>
		<% end %>
	
	<% end %>
<% end %>


<p id="notice"><%= notice %></p>

<h1>Listing Strains</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @strains.each do |strain| %>
      <tr>
        <td><%= strain.name %></td>

        <td><%= link_to 'Show', strain %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


